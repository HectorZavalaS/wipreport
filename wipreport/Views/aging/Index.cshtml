@model IEnumerable<wipreport.Models.siixsem_aging_rpt>

<div class="window" style="margin-top:15px;">
    <div class="window-title">
        <div class="window-content" id="viewRpt" style="color:black;">
            <div id="sectionData">
                <button type="button" class="btn btn-success" style="float:left;margin-left:15px;" id="btnDownload">Descargar reporte</button>
                <table id='tableRept' class='table table-striped table-bordered display nowrap' style='width:100%;font-size:10px;color:black;'>
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.LOCATOR)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.MAGAZINE)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.SCANED_SERIAL)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.MODEL)
                            </th>

                            <th>
                                @Html.DisplayNameFor(model => model.DJGROUP)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.QTY)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.SEMIFINISH)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.REVIEW)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.TYPE)
                            </th>

                            <th>
                                @Html.DisplayNameFor(model => model.USER_READ)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.QUARANTINE)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.STATUS)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.DATE_IN)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.DATE_OUT)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.CREATED_DATE)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.DAYS)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.AGING)
                            </th>
                        </tr>
                    </thead>
                    <tbody>

                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.LOCATOR)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.MAGAZINE)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.SCANED_SERIAL)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.MODEL)
                                </td>

                                <td>
                                    @Html.DisplayFor(modelItem => item.DJGROUP)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.QTY)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.SEMIFINISH)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.REVIEW)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.TYPE)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.USER_READ)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.QUARANTINE)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.STATUS)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.DATE_IN)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.DATE_OUT)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.CREATED_DATE)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.DAYS)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.AGING)
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
            <div id="sectionLoad">
                <div class="spinner-grow text-primary" role="status">
                    <span class="sr-only">Loading...</span>
                </div>
                <div class="spinner-grow text-secondary" role="status">
                    <span class="sr-only">Loading...</span>
                </div>
                <div class="spinner-grow text-success" role="status">
                    <span class="sr-only">Loading...</span>
                </div>
                <div class="spinner-grow text-danger" role="status">
                    <span class="sr-only">Loading...</span>
                </div>
                <div class="spinner-grow text-warning" role="status">
                    <span class="sr-only">Loading...</span>
                </div>
                <div class="spinner-grow text-info" role="status">
                    <span class="sr-only">Loading...</span>
                </div>
                <div class="spinner-grow text-light" role="status">
                    <span class="sr-only">Loading...</span>
                </div>
                <div class="spinner-grow text-dark" role="status">
                    <span class="sr-only">Loading...</span>
                </div>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        var table = $('#tableRept').DataTable({
            "paging": true,
            "searching": true,
            "lengthChange": false,
            "pageLength": 15,
            "info": false,
            "autoWidth": true,
            "ordering": true,
            "responsive": true,
            dom: 'Bfrtip',
            buttons: [
                'excel'
            ]
            //'language': { 'url': getVirtDir() + '/Scripts/Spanish.json' }
        });
        getAgingReport();
        //$("#sectionData").show("fade");
        //$("#sectionLoad").hide();
        //new $.fn.dataTable.FixedHeader(table);
    });
</script>
